{
  "minimum-stability": "stable",
  "name": "dukhanin/laravel-support",
  "description": "Laravel 5 additional support package",
  "keywords": [
    "laravel",
    "support",
    "array"
  ],
  "authors": [
    {
      "name": "Anton Dukhanin",
      "email": "anton.dukhanin@gmail.com"
    }
  ],
  "require": {
    "true/punycode": "^2.0",
    "jenssegers/date": "^3.2",
    "laravel/framework": "^5.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0",
    "mockery/mockery": "^1.0"

  },
  "extra": {
    "laravel": {
      "providers": [
        "Dukhanin\\Support\\Providers\\ServiceProvider"
      ],
      "aliases": {
        "Date": "Dukhanin\\Support\\Date"
      }
    }
  },
  "autoload": {
    "psr-4": {
      "Dukhanin\\Support\\": "src/",
      "Dukhanin\\Support\\Tests\\": "tests/"
    },
    "files": [
      "src/helpers.php"
    ]
  }
}
