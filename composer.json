{
  "minimum-stability": "stable",
  "name": "dukhanin/laravel-support",
  "description": "Laravel 5 additionall support package",
  "keywords": [
    "laravel",
    "support",
    "array"
  ],
  "authors": [
    {
      "name": "Anton Dukhanin",
      "email": "anton.dukhanin@gmail.com"
    }
  ],
  "require": {
    "php": "~7.0",
    "true/punycode": "~2.0",
    "jenssegers/date": "^3.2",
    "laravel/framework": "^5.4"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Dukhanin\\Support\\Providers\\ServiceProvider"
      ]
    }
  },
  "require-dev": {
    "phpunit/phpunit": "~5.7"
  },
  "autoload": {
    "psr-4": {
      "Dukhanin\\Support\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  }
}
